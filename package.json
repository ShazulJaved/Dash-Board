  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:inspect": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev:clean": "npm run clean && next dev",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start -p ${PORT:-3000}",
    "postbuild": "npx next-sitemap --config next-sitemap.config.js || exit 0",
    "lint": "next lint",
    "firebase:deploy": "firebase deploy",
    "typecheck": "tsc --noEmit",
    "setup-db": "tsx --require dotenv/config scripts/setup-db.ts",
    "seed-admin": "tsx scripts/seed-admin.ts",
    "clean": "rimraf .next && del /s /q .next"
  },
